{
  "name": "cerebro-api",
  "author": "Angael",
  "version": "0.2.0",
  "type": "module",
  "engines": {
    "node": ">= 18.12.1"
  },
  "scripts": {
    "start": "node build/src/main.js",
    "start:dev": "cross-env NODE_ENV=development tsc-watch --onSuccess \"npm run start\"",
    "start:prod": "cross-env NODE_ENV=production npm run start",
    "db:run": "docker run -it --name cerebro -v \"C:/cerebro/db:/var/lib/mysql\" -p 27017:27017 -e MYSQL_ROOT_PASSWORD=root -d mysql:latest",
    "db:start": "docker start cerebro",
    "prebuild": "rimraf build",
    "build": "cross-env NODE_ENV=production tsc -p tsconfig.json",
    "prisma:seed": "prisma db seed"
  },
  "prisma": {
    "seed": "npx tsx .\\prisma\\seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^4.8.0",
    "@types/fs-extra": "^9.0.13",
    "@vanih/dunes-node": "2.1.1",
    "async": "^3.2.4",
    "aws-sdk": "^2.1282.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "firebase-admin": "^11.4.1",
    "fs-extra": "^11.1.0",
    "imghash": "^0.0.9",
    "modern-async": "^1.1.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^2.3.3",
    "nanoid": "^4.0.0",
    "node-cache": "^5.1.2",
    "p-queue": "^7.3.0",
    "promise-retry": "^2.0.1",
    "sharp": "^0.31.3",
    "tslib": "~2.4",
    "winston": "^3.8.2",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/jest": "~29.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.18",
    "@types/sharp": "^0.31.1",
    "@typescript-eslint/eslint-plugin": "~5.39",
    "@typescript-eslint/parser": "~5.39",
    "eslint": "~8.24",
    "eslint-config-prettier": "~8.5",
    "eslint-plugin-jest": "~27.1",
    "jest": "~29.1",
    "prettier": "~2.7",
    "prisma": "^4.8.0",
    "rimraf": "~3.0",
    "ts-jest": "~29.0",
    "ts-node": "^10.9.1",
    "tsc-watch": "^5.0.3",
    "tsutils": "~3.21",
    "typescript": "^4.9.4"
  }
}
